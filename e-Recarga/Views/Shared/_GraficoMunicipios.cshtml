@model IEnumerable<e_Recarga.DTOs.MunicipioMaisUtilizados>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Municipio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nUtilizacoes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Municipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nUtilizacoes)
        </td>
    </tr>
}

</table>
